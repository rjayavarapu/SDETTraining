
public class NumberCals {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		printName();
		addNumbers(5,6);
		multiplyNumbers(5,6);
	}
	
	private static void printName()
	{
		System.out.println("My Name is Tim");
		
	}
	
	private void int addNumbers(int number1,int number2)
	{
		System.out.println("Addition of Two Numbers" + number1 + number2);
		
	}
	
	private void int multiplyNumbers(int number1, int number2)
	{

		System.out.println("Addition of Two Numbers" + number1 * number2);
	}
	

}
