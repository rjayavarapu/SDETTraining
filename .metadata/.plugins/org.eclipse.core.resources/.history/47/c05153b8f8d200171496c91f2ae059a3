
public class NumberCals {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		printName();
		addNumber(5,6;)
	}
	
	private static void printName()
	{
		System.out.println("My Name is Tim");
		
	}
	
	private int addNumbers(int number1,int number2)
	{
		System.out.println("Addition of Two Numbers" + number1 + number2);
		
	}
	
	private int multiplyNumbers(int number1, int number2)
	{

		System.out.println("Addition of Two Numbers" + number1 * number2);
	}
	

}
