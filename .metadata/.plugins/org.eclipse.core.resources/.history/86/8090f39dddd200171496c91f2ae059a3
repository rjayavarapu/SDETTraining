
public class Days {

public static void main(String[] args)
{
	
	String dayOfWeek;
	
	switch(dayOfWeek)
	{

	case "Monday":
		System.out.println("Today is Tueday");
	break;
	case "Tuesday":
		System.out.println("Today is Tueday");
		break;
	default:
		System.out.println("Invalid Week");
		
	
	}
}

}
